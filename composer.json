{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.1",
        "easyrdf/easyrdf": "^0.9.1",
        "elasticsearch/elasticsearch": "5.3.*",
        "guzzlehttp/guzzle": "~6.3",
        "linked-swissbib/adapterESdev": "dev-updateOngr2Version5",
        "ml/json-ld": "^1.0",
        "ongr/elasticsearch-dsl": "5.0.*",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.2.0",
        "symfony/templating": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "LinkedSwissbibBundle\\": "src/"

        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "linked-swissbib/adapterES",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/linked-swissbib/adapterElasticsearch.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0" : {
                        "ElasticsearchAdapter" : "src"
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "linked-swissbib/adapterESdev",
                "version": "dev-updateOngr2Version5",
                "source": {
                    "url": "https://github.com/linked-swissbib/adapterElasticsearch.git",
                    "type": "git",
                    "reference": "updateOngr2Version5"
                },
                "autoload": {
                    "psr-0" : {
                        "ElasticsearchAdapter" : "src"
                    }
                }
            }
        }

    ],



    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C92K4R3DS2GMBDP2KHRY5SD3",
            "allow-contrib": false
        }
    }
}
